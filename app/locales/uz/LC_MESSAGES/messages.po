msgstr "Iltimos, to'g'ri yosh chegarasini belgilang"

#: shared/validators.py:147
msgid "Age can't be lower than {min_age}"
msgstr "Yosh {min_age} dan kichik bo'lishi mumkin emas"

#: shared/validators.py:151
msgid "Age can't be higher than {max_age}"
msgstr "Yosh {max_age} dan katta bo'lishi mumkin emas"

#: shared/validators.py:160
msgid "Minimum age needs be to lower than maximum age"
msgstr "Minimum yosh maksimum yoshdan kichik bo'lishi kerak"

#: shared/validators.py:167
msgid "Video duration can't be longer than {max_duration} seconds"
msgstr "Video davomiyligi {max_duration} dan uzun bo'lishi mumkin emas"

#: shared/validators.py:176
msgid "Message text cannot be empty"
msgstr "Xabar matni bo'sh bo'lishi mumkin emas"

#: shared/validators.py:179
msgid "Message text must be less than {max_length} characters long"
msgstr "Xabar matni uzunligi {max_length} dan kam bo'lishi kerak"

